                            android:textSize="@dimen/speedMeasure"
                            android:textColor="@color/dlText"
                            />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:id="@+id/ulArea"
